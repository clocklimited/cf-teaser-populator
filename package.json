    "should": "2",
    "istanbul": "0",
    "jshint": "2",
    "jshint-full-path": "0",
    "service-locator": "0.0.2",
    "fleet-street": "~2.0.1",
    "mongodb": "~1.3.20",
    "save": "0.0.20",
    "crud-service": "0.0.5",
    "mc-logger": "0.0.0",
    "save-mongodb": "0.0.11",
    "cf-dedupe-list-aggregator": "0.0.3",
    "schemata": "~1.0.0"
  }
